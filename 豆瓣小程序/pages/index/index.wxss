/**index.wxss**/
.serch{
  background-color:#00ff00;
  height: 80rpx; 
  display: flex;
  justify-content: center;
  align-items: center;
}
.serch view{
  width: 90%;
  height: 80%;
  background-color: #fff;
  display: flex;
  align-items: center;
  border-radius: 35rpx;
  padding-left: 30rpx;
}
.serch view input{
  width: 90%;
  padding-left: 20rpx;
}
.tableheader{
  height: 80rpx;
  background-color: #fff;
  padding: 0 30rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.tableheader text{
  color: #00ff00;
  font-size: 30rpx;
}
.listbox{
  display: flex;
  flex-wrap: nowrap;
  overflow-y: hidden;
  margin-bottom: 50rpx;
}
.listbox::-webkit-scrollbar{
    display: none;
}
.listinner{
  margin: 8rpx;
}
.listinner image{
  width: 200rpx;
  height: 286rpx;
}
.listinner view:nth-of-type(1){
  width: 200rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  text-align: center;
}
.listinner view:nth-of-type(2){
  text-align: center;
}
.listinner view:nth-of-type(2) .active{
  color: gold;
}